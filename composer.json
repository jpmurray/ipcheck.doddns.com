{
  "name": "jpmurray/ipcheck.doddns.com",
  "description": "Returns the current IP of a user for usage with DODDNS",
  "keywords": [
    "digitalocean",
    "digital ocean",
    "ddns",
    "doddns",
    "ip"
  ],
  "homepage": "https://github.com/jpmurray/ipcheck.doddns.com",
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/jpmurray/ipcheck.doddns.com/issues",
    "source": "https://github.com/jpmurray/ipcheck.doddns.com"
  },
  "authors": [
    {
      "name": "Jean-Phlippe Murray",
      "email": "jp@atomescroch.us"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "vectorface/whip": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
